   "source": [
    "# 첫번째 문장(string1)에 대해 띄어쓰기 영역 정보 표시\n",
    "word_tokens, char_to_word = _tokenize_whitespace(string1)\n",
    "for c, i in zip(list(string1), char_to_word):\n",
    "    print(f'\\'{c}\\' : {i}')\n",
    "    \n",
    "word_tokens, char_to_word"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "f8ed3b1b",
   "metadata": {},
   "outputs": [
